class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        output_list=[]
        for i in range(0,len(nums)):
            for j in range(i+1,len(nums)):
                if nums[i]+nums[j]==target:
                    output_list.append(i)
                    output_list.append(j)
        return output_list            
